-- vars
function _init()
	p = {
		x=0,
		y=0,
		dx=0,
		dy=0,
		acc=0.3,
		dx_max=2,
		dy_max=4,
		v_boost=6,
		sprite=1,
		w=16,
		h=16,
		flp=false,
		anim=0,
		landed=true,
		jumping=false,
		falling=false,
		sliding=false,
		running=false
		}
	
	world={
		gravity=0.5,
		friction=0.85,
		start=0,
		ending=256
	}
	
	cam={
	x=0
	}
	
end
